{
  "name": "performance-decorators",
  "version": "3.0.4",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "test": "jest --config=jest.config.base.ts",
    "test:node": "jest --config=jest.config.node.ts",
    "test:browser": "jest --config=jest.config.browser.ts",
    "publish-all": "pnpm -r publish --filter './packages/{node,browser}' --access public"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.3.3"
  }
}
